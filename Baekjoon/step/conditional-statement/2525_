A, B = map(int, input().split()) # 시, 분
timer = int(input()) # 요리하는데 필요한 시간

A += timer // 60
B += timer % 60 
   
   B = B // 60 
   A += B
   
   B = B % 60
   A += B
   
   print(A, B)

else: # 분이 60분 미만일 때
    B += C
    print(A, B)

A, B = map(int, input().split()) # 시, 분
timer = int(input()) # 요리하는데 필요한 시간

if B + timer >= 60: # 분이 60분 이상일 때
   B += timer
   
   B = B // 60 
   A += B
   
   B = B % 60
   A += B
   
   print(A, B)

else: # 분이 60분 미만일 때
    B += C
    print(A, B)